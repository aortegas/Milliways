<?xml version="1.0" encoding="utf-8"?>
<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    android:layout_margin="@dimen/margin_32dp"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".activity.ActivitySettingsDetailTable">

    <TextView
        android:id="@+id/activity_settings_detail_textnum_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:paddingTop="@dimen/margin_10dp"
        android:text="@string/NUMERO_PLATOS"/>

    <TextView
        android:id="@+id/activity_settings_detail_number_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_5dp"/>

    <TextView
        android:id="@+id/activity_settings_detail_texttotal_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textStyle="bold"
        android:paddingTop="@dimen/margin_32dp"
        android:text="@string/TOTAL_IMPORTE"/>

    <TextView
        android:id="@+id/activity_settings_detail_total_id"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:paddingTop="@dimen/margin_5dp"/>

</LinearLayout>



