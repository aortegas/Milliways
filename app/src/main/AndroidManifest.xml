<?xml version="1.0" encoding="utf-8"?>
<manifest package="io.keepcoding.milliways"
          xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Register access to Internet. -->
    <uses-permission android:name="android.permission.INTERNET"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.ActivityMain">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".activity.ActivityRestaurantList">
        </activity>
        <activity android:name=".activity.ActivityTableList">
        </activity>
        <activity android:name=".activity.ActivityPlateList">
        </activity>
        <activity android:name=".activity.ActivityPlateDetail">
        </activity>
    </application>

</manifest>
